<h1>Sensor Information</h1>
<table>
  <thead>
    <tr>
    <th>Sensor number</th>
    <th>Name</th>
    <th>Current battery Voltage</th>
    <th>Time since last measurment</th>
    </tr>
  </thead>

  <tbody>
    <% @nodes.each do |node| %>
      <tr>
        <td><%= node.node_id%></td>
        <td><%= node.name%></td>
        <td><%= node.last_reading%></td>
        <td><%= link_to 'Rename', node %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>