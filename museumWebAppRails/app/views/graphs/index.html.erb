
<p id="notice"><%= notice %></p>

<script src="application.js"></script>

<h1>Today's Measurements</h1>

<%= for i in 1..numNodes do %>

<%=

  line_chart getData(i),
  title: "Sensor " + i.to_s,
  xtitle: "Time",
  ytitle: "Temperature/Humidity",
  legend: true,
  legend: "bottom",
  download: true
  
%>

</br>
<hr>
</br>

<p hidden><% end %></p>