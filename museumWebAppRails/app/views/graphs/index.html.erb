
<p id="notice"><%= notice %></p>

<script src="application.js"></script>

<h1>Today's Measurements</h1>

<%= @nodeList.each do |node|%>

<%=

  line_chart getData(node),
  title: nodeName(node),
  xtitle: "Time",
  ytitle: "Temperature/Humidity",
  legend: true,
  legend: "bottom",
  download: true
  
%>

</br>
<hr>
</br>

<p hidden><% end %></p>

<%=

	line_chart allTemp,
	title: "All Temperatures",
	xtitle: "Time",
	ytitle: "Temperature (F)",
	legend: true,
	legend: "bottom",
	download: true

%>

<%=

	line_chart allHum,
	title: "All Humidities",
	xtitle: "Time",
	ytitle: "Humidity (%)",
	legend: true,
	legend: "bottom",
	download: true

%>